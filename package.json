    "axios": "^1.5.1",
    "date-fns": "^2.30.0",
    "echarts": "^5.4.3",
    "echarts-gl": "^2.0.9",
    "element-plus": "^2.6.3",
    "pinia": "^2.1.6",
    "screenfull": "^6.0.2",
    "swiper": "^11.0.5",
